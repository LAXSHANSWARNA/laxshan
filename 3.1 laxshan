date_of_birth= int(input("please type in your date of birth"))
month_of_birth= int(input("please enter month you born"))
year_of_born= int(input("please enter the year of you born"))
total_number_of_birth=(date_of_birth+month_of_birth+year_of_born)
print(total_number_of_birth)
